{
  "version": 2,
  "name": "blog-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/blog-app/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/blog-app/browser",
  "framework": "angular",
  "functions": {
    "src/server.ts": {
      "runtime": "nodejs20.x"
    }
  }
}
